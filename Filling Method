class Filling
{
  public static void main(String[] args)
  {
    String[] birds = new String[5];
    birds[0] = "pigeon";
    birds[1] = "chicken";
    birds[2] = "ostrich";
    birds[3] = "sparrow";
    
    int length = birds.length;
    
    int i=0;
    while (i < length)
    {
      System.out.println(birds[i]);
      i += 1;
    }
  }
}
